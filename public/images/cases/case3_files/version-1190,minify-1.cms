            var containsSpecialChars = function(str){
              var specialChars = `\`!@#$%^&*()_+\-=\[\]{};':"\\|,.<>\/?~`;
              var result = specialChars.split('').some(specialChar => {
                if (str.includes(specialChar)) {
                  return true;
                }
                return false;
              });
            
              return result;
            }
            
            var checkUserName = function(str){
