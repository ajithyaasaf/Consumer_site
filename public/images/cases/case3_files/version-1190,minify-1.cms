                if( !sidebar.length ){
                    return dimensions;
                }
                
                dimensions.width = sidebar.width() + "px";
                
                var wrappperOffset = 0;
                var wrapper = sidebar.closest('.wrapper').length ? sidebar.closest('.wrapper') : $(".wrapper");
                if( wrapper.length ){
                    wrappperOffset = wrapper.offset().left + parseInt( wrapper.css("padding-left") );
