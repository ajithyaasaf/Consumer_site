            if(window.plsObj.isToiPPSUser || window.plsObj.isCredPPSUser || isUserPrc()){
                params = 'toipr=2&primetemplate=1'; 
            }  
            url = removeParam('toipr', url);
            url = removeParam('primetemplate', url);
            
            // Logout case, we should remove toipr=2
            if (url.indexOf('?') > -1){
                url += '&';
            }
            else{
                url += '?';
            }
            url += params;
            return url;
        };
        
