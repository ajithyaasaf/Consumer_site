        mod_login.getFirstAndLastName = function (name) {
            var nameArr = [];
            var nameObj = {firstName: '', lastName: ''};
            if(name && name.length > 0) {
                name = name.replace(/  +/g, ' ');
                nameArr = name.split(' ');
                nameObj.firstName = nameArr[0] || '';
                if(nameArr.length > 1) {
                    nameArr.splice(0,1);
                    nameObj.lastName = nameArr.join(' ');
                }
            }
            
            return nameObj;
        };
        
