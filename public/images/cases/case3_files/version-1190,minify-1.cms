            if (navObj.windowTop > navObj.stickyTop) {
                $('#navigation').addClass('sticky');
            } else {
                $('#navigation').removeClass('sticky');
            }
            
            var st = $(this).scrollTop();
            if (st > lastScrollTop){
                scrollDir = 'downwards';
                $('body').removeClass('scroll-up').addClass('scroll-down');    
                
             } else {
