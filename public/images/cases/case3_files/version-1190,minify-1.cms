    var getPrcCookieValue = function () {
                        var __prc;
                        __prc = document.cookie.match(new RegExp(' prc=([^#]+)'));
                        if (!__prc) {
                            __prc = document.cookie.match(new RegExp('prc=([^#]+)'));
                        }
                        return parseInt(__prc && __prc[1] ? __prc[1] : 0);
                    }
    var isUserPrc = function () {
