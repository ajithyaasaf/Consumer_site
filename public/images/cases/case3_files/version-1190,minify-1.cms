            if( vidIframe
                && vidIframe[0]
                && vidIframe[0].contentWindow
                && vidIframe[0].contentWindow.TimesApps.VideoPlayer2.getPlayer("VOD")
                && vidIframe[0].contentWindow.TimesApps.VideoPlayer2.getPlayer("VOD").pause
