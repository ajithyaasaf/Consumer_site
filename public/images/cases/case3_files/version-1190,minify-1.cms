           if( msid ){
               $msgTemplate = config.$msgTemplate.clone();
               defaultUrl = $msgTemplate.find('.jNtThumb').eq(0).data('imgsrctemplate') || "";
               thumbUrl = defaultUrl.replace(regex, msid);
           }
