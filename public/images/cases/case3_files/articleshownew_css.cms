}


@media screen and (min-width: 768px) and (max-width: 1100px) {
    /*.sidebar{width:655px!important;}*/
    .title_section{position:relative;}
    .show_wdgt{float:left; margin:20px 3px 0 4px;}
    /*.main-content{width:655px!important;}*/
    .leftsocial{top:inherit; left:300px; bottom:3px; position:absolute;}
    .topsocial .print{display:none;}
    .leftsocial span{float:left; margin:0 2px;}
    .leftsocial.sticky{position:absolute; top:inherit; margin-left:0; width:auto; left:195px;}
    .actions .email, .actions .print, .actions .fontd, .actions .fonti{margin:0 2px;}
    .sticky_adwidget, .maxbottom_ad, .maxbottom_rhs, .sticky_rhs{position:static;}
}
@media screen and (max-width: 999px) and (min-width: 768px){
    #content .wrapper, .navbdcrumb {
      width: 768px;
      margin: 0 auto;
    }
}

@media print {
    @page {size: landscape;}
    .ad728, .breaking-news,#minitv, #header, #navigation, .sidebar, #footer,.toiwidgets_social, .googlead, .follow_show,.columbia_div, .comments{
        display:none;
    }
    .main-content .infographics_syn,.print_header{
        display:block;
        width:100% !important;
    }
}

