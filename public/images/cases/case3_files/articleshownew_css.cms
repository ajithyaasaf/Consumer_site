.related_articles a:hover{text-decoration:none;}
.related_articles{margin: 30px 0;border-bottom: 1px solid #eee;padding-bottom: 20px;}
.section_header{color: #000;text-transform: uppercase;border-bottom: 4px solid #000;padding-bottom: 6px;display: inline-block;font-family: inherit;font-size: 18px;margin-bottom: 15px;}
.related_articles li{width:155px;display:inline-block; vertical-align:top;margin:0 10px 0 0;}
.related_articles ul li img{width: 155px;height: 116px;border: 1px solid #ddd;}
.related_articles .more-link{color: #595959;font: bold 14px/14px 'Montserrat', Arial, Helvetica, sans-serif;margin: 0 0 10px 0;padding-top:12px;text-transform: uppercase;float:right;}
.related_articles ul li .desc a{font-size: 13px;padding: 5px 0;line-height: 1.3;color: #333; display:inline-block;}

.ctn_ads_twins{margin-bottom:15px;}
.ctn_ads_twins ul li{max-width: 240px;display: inline-block;vertical-align: top;min-width: 50px;width: 31.8%;box-sizing: border-box;position: relative;margin: 0 0 0 2.3%;}
.ctn_ads_twins ul li:first-child{margin:0;}
.ctn_ads_twins ul li a:hover{text-decoration:none;}
.ctn_ads_twins .ad_img .ad_heading{font-family: inherit;color: #333;padding: 6px 0 0;text-align: left;line-height: 1.25;font-size: 13px;max-height: 49.0;overflow: hidden;font-weight: bold;display: block;}
.ctn_ads_twins .ad_img .ad_brand{font-family: inherit;color: #999;padding: 2px 0 0;text-align: left;font-size: 12px;display: block;}
.ctn_ads_twins .ad_img .ad_brand, .ctn_ads_rhs .ad_brand{font-family: inherit;color: #999;padding: 2px 0 0;text-align: left;display: block;font:normal 13px/14px 'Montserrat', Arial, Helvetica, sans-serif;}

