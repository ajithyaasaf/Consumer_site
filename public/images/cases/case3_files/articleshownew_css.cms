/*Video Section CSS*/

/*------ Article Video -------*/
.articlevideo.abovearticle{margin-bottom:30px;}
.articlevideo{position:relative;  background:#000; }
.articlevideo .toppart{min-height:400px; position:relative;}
.articlevideo .preloader{position:relative;  max-height:400px; overflow:hidden;}
.articlevideo_pic{display:block; margin:0 auto; width:100%;}
.articlevideo .title{position:absolute; left:0px; top:140px; z-index:6; text-align:left; height: 138px; padding:20px 20px 20px 158px; color: #fff; font: normal 30px/36px 'Montserrat', Arial, Helvetica, sans-serif; background: #000; background: rgba(0,0,0,0.7); box-sizing:border-box;}
.articlevideo .title span{display:table-cell; height:98px; vertical-align:middle;}
.articlevideo .duration{ bottom: 20px; color: #fefefe; left: 20px;  position: absolute; z-index: 6; font-size:12px; text-shadow:1px 2px 5px #000;}
.articlevideo embed, .articlevideo object{height:400px;}
.articlevideo .play-icon{cursor:pointer; display:block; z-index:10; width:173px; height:173px; background-position:0 -100px; transition:all .2s ease; -moz-transition:all .2s ease; -webkit-transition:all .2s ease; position:absolute;}
.articlevideo .play-icon, .abovearticle .play-icon{transform: scale(.7,.7); -ms-transform: scale(.7,.5); -webkit-transform: scale(.7,.7);}
.articlevideo:hover .play-icon{transform: scale(1,1); -ms-transform: scale(1,1); -webkit-transform: scale(1,1);}
.abovearticle:hover .play-icon{transform: scale(1,1); -ms-transform: scale(1,1); -webkit-transform: scale(1,1);}
.abovearticle .play-icon{transform: scale(.7,.7); -ms-transform: scale(.7,.7); -webkit-transform: scale(.7,.7);}
.articlevideo .vidbox { z-index:11; position:absolute; top:0px; width:100%; }
.articlevideo .vidbox .dvideo { position:relative; height:400px!important; }


.articlevideo .close{position:absolute; background:url(/photo/41603652.cms) no-repeat -222px -45px;
    width:32px; height:32px; position:absolute; z-index:50; top:-10px; right:-10px; cursor:pointer; display:block; display:block;}
.articlevideo .vidloader { display:none; }
.articlevideo.loadingvid .vidloader{ display:block; left: 50%; margin-left: -64px; margin-top: -64px; position: absolute; top: 50%;z-index: 20;}
.safari .topimgbox .inner{transition:none; -moz-transition:none; -webkit-transition:none;}



.articlevideo .related-videos{background:#222; padding:10px 5px; position:relative;  margin-bottom:9px;}
.articlevideo .related-videos h4{color:#fff; font-size:16px; font-weight:normal; line-height:18px; margin-bottom:8px;  }
.articlevideo .related-videos .slider{position:relative; overflow:hidden; padding:0 19px; height:68px; }
.articlevideo .related-videos .slider .slider-inner{overflow:hidden;}
.articlevideo .related-videos .slider .slider-wrapper{width:3000px; position:relative; }
