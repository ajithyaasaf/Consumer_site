/*Slideshow section css*/
.slideshowcontainer {position: relative; margin: 20px auto;}
.slideshowbox{position:relative;}
.slideshowbox > h3{display:none;}
.slideshowbox .pagination{position:absolute; z-index:20; right:15px; bottom:-31px; font-size:14px; color:#fff; line-height:15px;}
.slideshowbox .pagination .seperator:before{content:'/'; color:#fff; display:inline-block; margin:0 2px;}
.slideshowbox .slidesouter{width:100%; overflow:hidden; min-height:436px;}
.slideshowbox .slides{width:100000px; }
.slideshowbox .slide{ float:none; display:table-cell; vertical-align:middle; background:#000; text-align:center; width:655px;}
.slideshowbox .slide img{width:100%; }
.slideshowcontainer .infobox{background:#000; padding:10px 20px 2px; overflow:hidden; height:auto;}
.slideshowcontainer .infoslides{width:100000px;}
.slideshowcontainer .infoslide{ float:left; margin-bottom:10px;}
.slideshowcontainer .infoslide h4{font-size:20px; color:#fff; line-height:25px; font-weight:normal; margin:0 80px 0 20px; cursor:pointer;}
.slideshowcontainer .infoslide .slidecaption{color:#999; font-size:15px; line-height:20px; height:150px; overflow:hidden; margin:10px 0 0 17px;}
.infobox .infoouter{overflow:hidden;} /*height:208px;*/
